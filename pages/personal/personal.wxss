/* pages/personal/personal.wxss */
.userInfoBox{
    padding: 0 25rpx;
    height: 480rpx;
    background-image: url("data:image/jpg;base64,");
    background-position:center;  /** 居中*/
    background-repeat:no-repeat; /** 不重复*/
    background-size:cover; /**等比例缩放*/
}

.userPortrait{
    padding-top: 100rpx;
    padding-bottom: 30rpx;
    display: flex;
    align-items: center;
}
.userPortrait image {
    width: 120rpx;
    height: 120rpx;
    border: 5rpx solid #fff;
    border-radius: 50%;
}
.userPortrait text {
    font-size: 40rpx;
    font-weight: 500;
    margin: 0 30rpx;
    color: #333;
}
.userVip {
    position: relative;
    height: 220rpx;
    padding: 0 20rpx;
    background-color:rgba(0, 0, 0, 0.7);
    border-radius: 30rpx;
    color: #d3c597;
    overflow: hidden;
}
.bgikon {
    position: absolute;
    right: 0;
    top: 50rpx;
    width: 350rpx;
    height: 100%;
    z-index: 0;
}
.vipInfo {
    margin-top: 10rpx;
    display: flex;
    justify-content: space-between;     
}
.openVip {
    background-color: #f9e6af;
    color: #36343c;
    padding: 4rpx 20rpx;
    border-radius: 30rpx;
    font-size: 22rpx;
}
.iconVip .iconTitle {
    font-size: 26rpx;
    padding-left: 20rpx;
}
.userName {
    padding-top: 30rpx;
    font-size: 40rpx;
    font-weight: 500;
}
.VipIntroduce {
    padding-top: 20rpx;
    font-size: 26rpx;

}

/* 内容区域 */
.content {
    width: 100%;
    position: absolute;
    top: 450rpx;
    bottom: 0;
    background-color: #f5f5f5;
}
.arc {
    position: absolute;
    top: -34rpx;
    width: 100%;
    height: 36rpx;
}
.menu {
    padding: 0 25rpx;
}
.navigation-box {
    display: flex;
    justify-content: space-around;
    padding: 30rpx 0;
    margin-bottom: 20rpx;
    background-color: #fff;
    border-radius: 25rpx;
}
.navigation-item {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.navigation-item .navigation-item-icon {
    font-size: 50rpx;
    color: #d23e32;
}
.navigation-item .navigation-item-title {
    font-size: 20rpx;
    color: #333;
    padding-top: 10rpx;
}

/* 菜单选项 */
.menu-box {
    padding: 10rpx 10rpx;
    background-color: #fff;
    border-radius: 25rpx;  
}
.menu-item {
    display: flex;
    justify-content: space-between;
    padding: 30rpx 0;
    border-bottom: 2rpx solid #eee;
    font-size: 28rpx;
    padding-right: 20rpx;
}
.menu-item:first-child {
    display: block;
    font-size: 34rpx;
    padding-top: 10rpx;
}
.menu-item .menu-item-title {
    display: block;
    margin-bottom: 10rpx;
}
.menu-item:last-child {
    border-bottom: 0;
}

/* 最近播放 */
.recently-played {
    height: 150rpx;
    display: flex;
}
.recently-played image {
    flex-shrink: 0;
    width: 150rpx;
    height: 150rpx;
    border-radius: 10rpx;
    margin-left: 10rpx;
}
.recently-played image:first-child {
    margin-left: 0;
}
.recently-played-hint {
    font-size: 26rpx;
    color: #ccc;
}